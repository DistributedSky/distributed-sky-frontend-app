<div class="accountAdd__form-block">
    <form class="accountAddForm__input">
        {{#each input }}
            {{{ this }}}
        {{/each}}
    </form>
    <div class="accountAddForm__admission">
        <div class="accountAddForm__header-enter">
            {{{ StandardButton }}}
        </div>
    </div>
    <div class="result-block {{#unless visibility }}result-block-display-none{{/unless}}">
        {{ result }}
    </div>
    <div class="account-add__info {{#unless visibility}}info-display-none{{/unless}}">
        {{ info }}
    </div>
    <div class="block-hash {{#unless visibility}}block-hash-display-none{{/unless}}">
        {{ blockHash }}
    </div>
</div>

